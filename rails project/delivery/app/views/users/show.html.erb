<p id="notice"><%= notice %></p>



<% if flash[:message] %>
  <div class="center alert">
    <p><%= flash[:message] %></p>
  </div>
<% end %>

<div class="center">

  <div class="info">
    <% if @user.admin %>
      <p><strong>ADMIN</strong></p>
    <% end %>
    <% @user.meals.each do |meal| %>
    <%# meal. %>
  <% end %>
    <p><strong>User Name:</strong> <%= @user.name %></p>
    <p><%= link_to 'create meal', new_meal_path %></p>
  </div>
</div>
